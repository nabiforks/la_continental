<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="product_template_orm_view" model="ir.ui.view">
            <field name="name">fleet.template.tree.view</field>
            <field name="model">fleet.repair</field>
            <field name="inherit_id" ref="car_repair_industry.view_fleet_repair_tree"></field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                    <field name="license_plate" string='Placa'></field>                   
                </xpath>
            </field>
        </record>
    </data>
</odoo>

